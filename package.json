{
	"name": "log-booster",
	"displayName": "log-booster",
	"description": "An VS code extension to add frequently used log statements",
	"version": "0.0.1",
	"publisher": "nvinayvarma189",
	"author": {
		"name": "Vinay Varma",
		"email": "vinay.n.varma189@gmail.com"
	},
	"engines": {
		"vscode": "^1.66.0"
	},
	"bugs": {
		"url": "https://github.com/nvinayvarma189/log-booster/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/nvinayvarma189/log-booster.git"
	},
	"categories": [
		"Other",
		"Debuggers",
		"Snippets"
	],
	"galleryBanner": {
		"color": "#136207",
		"theme": "dark"
	},
	"keywords": [
		"log-booster",
		"logger",
		"print"
	],
	"activationEvents": [
		"onCommand:log-booster.insertStatements"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "log-booster.insertStatements",
				"title": "Log Booster"
			}
		],
		"keybindings": [
			{
				"command": "log-booster.insertStatements",
				"key": "ctrl+alt+l",
				"mac": "cmd+alt+l",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.66.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.16.0",
		"@typescript-eslint/parser": "^5.16.0",
		"eslint": "^8.11.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.2",
		"typescript": "^4.5.5",
		"ts-loader": "^9.2.8",
		"webpack": "^5.70.0",
		"webpack-cli": "^4.9.2",
		"@vscode/test-electron": "^2.1.3"
	}
}